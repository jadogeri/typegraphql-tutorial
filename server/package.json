{
  "name": "server",
  "version": "1.0.0",
  "description": "node project utilizing Typegraphl to fetch data",
  "keywords": [
    "typegraphql",
    "typeorm",
    "typescript",
    "node",
    "apollo",
    "database"
  ],
  "homepage": "https://github.com/jadogeri/typegraphql-tutorial#readme",
  "bugs": {
    "url": "https://github.com/jadogeri/typegraphql-tutorial/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadogeri/typegraphql-tutorial.git"
  },
  "license": "MIT",
  "author": "Joseph Adogeri",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "start": "tsx watch src src/index.ts",
    "test": "jest --coverage",
    "dev": "cross-env NODE_ENV=development tsx watch src src/index.ts",
    "build": "npm run swagger:generate && tsc && tsc-esm-fix --target='dist'",
    "dist":  "node dist/src/index.js",
    "typeorm": "typeorm-ts-node-commonjs --dataSource ./src/configs/typeOrm.config.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create ./src/migrations/InitialSchemaSetup",
    "migration:generate": "npx tsx ./node_modules/typeorm/cli.js migration:generate ./src/migrations/InitialSchemaSetup --dataSource ./src/configs/typeOrm.config.ts --pretty",
    "migration:run": "npx tsx ./node_modules/typeorm/cli.js migration:run --dataSource ./src/configs/typeOrm.config.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert --dataSource ./src/configs/typeOrm.config.ts",
    "swagger:generate": "tsoa spec-and-routes",
    "src:generate": "npx tsc-esm-fix --src='server/src/**/*' --ext='.js'"
  },
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@libsql/sqlite3": "^0.3.1",
    "@tsoa/runtime": "^7.0.0-alpha.0",
    "better-sqlite3": "^12.6.0",
    "body-parser": "^2.2.2",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "inversify": "^7.11.0",
    "inversify-binding-decorators": "^4.0.0",
    "jest": "^30.2.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.4.6",
    "tsoa": "^7.0.0-alpha.0",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.28",
    "typeorm-extension": "^3.7.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
